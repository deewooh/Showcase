<%- include('../partials/header') %>

<%- include('../partials/navigation_admin') %>

<div class="container">
  <h1 class="portal-h1">Pending Registrations</h1>
  <table class="table table-striped table-hover">
      <thead>
        <tr>
          <th scope="col">Registration ID</th>          
          <th scope="col">Customer ID</th>
          <th scope="col">VIN</th>
          <th scope="col">Status</th>
        </tr>
      </thead>
      <tbody>
      <% json_data = JSON.parse(data); %>
      <%if (json_data.length >0) { %>
          <% json_data.forEach(function(item){ %>
              <tr>
                <td><a href="/registrationdetail/<%= item.Record.registrationID %>"><%= item.Record.registrationID %></a></td>                
                <td><%= item.Record.person %></td>
                <td><%= item.Record.vin %></td>
                <td><%= item.Record.status %></td>
              </tr>
          <% })
      } else{ %>
              <tr>
                  <td colspan="5">No Record Found</td>  
              </tr>
      <% } %>
      </tbody>
    </table> 
</div>

<%- include('../partials/footer') %>