<%- include('partials/header') %>

<%- include('partials/navigation_cust') %>

<div class="container">
    <h1 class="portal-h1">Vehicles Test Page</h1>
    <table class="table table-striped table-dark table-hover">
        <thead>
          <tr>
            <th scope="col">VIN</th>
            <th scope="col">Make</th>
            <th scope="col">Model</th>
            <th scope="col">Body Style</th>
            <th scope="col">Model Year</th>
          </tr>
        </thead>
        <tbody>
        <% json_data = JSON.parse(data); %>
        <%if (json_data.length >0) { %>
            <% json_data.forEach(function(item){ %>
                <tr>
                  <td><%= item.Record.vin %></td>
                  <td><%= item.Record.make %></td>
                  <td><%= item.Record.model %></td>
                  <td><%= item.Record.body_style %></td>
                  <td><%= item.Record.model_year %></td>
                </tr>
            <% })
        } else{ %>
                <tr>
                    <td colspan="5">No Record Found</td>  
                </tr>
        <% } %>
        </tbody>
      </table> 
</div>

<%- include('partials/footer') %>